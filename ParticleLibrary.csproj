<?xml version="1.0" encoding="utf-8"?>
<Project Sdk="Microsoft.NET.Sdk">
	<Import Project="..\tModLoader.targets" />
	<PropertyGroup>
		<AssemblyName>ParticleLibrary</AssemblyName>
		<TargetFramework>net6.0</TargetFramework>
		<PlatformTarget>AnyCPU</PlatformTarget>
		<LangVersion>latest</LangVersion>
		<GeneratePackageOnBuild>true</GeneratePackageOnBuild>
	</PropertyGroup>
	<PropertyGroup>
		<GenerateDocumentationFile>true</GenerateDocumentationFile>
	</PropertyGroup>
	<ItemGroup>
	  <Compile Remove="ExampleParticles\EmberParticle.cs" />
	  <Compile Remove="ExampleParticles\GlowParticle.cs" />
	  <Compile Remove="ExampleParticles\RainbowParticle.cs" />
	</ItemGroup>
	<ItemGroup>
	  <None Include="ExampleParticles\EmberParticle.cs" />
	  <None Include="ExampleParticles\GlowParticle.cs" />
	  <None Include="ExampleParticles\RainbowParticle.cs" />
	</ItemGroup>
	<ItemGroup>
		<PackageReference Include="tModLoader.CodeAssist" Version="0.1.*" />
	</ItemGroup>
</Project>